{

	"credentials" : {
		"username" : "",
		"password" : ""
	},

	"defaults" : {
		"url" : "www.assembla.com"
	},

	"services" : {

		"list_of_milestones" : {
			"uri" : "spaces/${space_id}/milestones/",
			"type" : "GET"
		},

		"show_milestone" : {
			"uri" : "spaces/${space_id}/milestones/${milestone_id}",
			"type" : "GET"
		},

		"my_spaces_list" : {
			"uri" : "spaces/my_spaces",
